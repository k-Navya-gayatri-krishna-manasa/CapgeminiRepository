<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">
	<bean class="com.cg.service.HelloService" id="helloService"></bean>
	<bean class="com.cg.service.Employee" id="emp">
		<property name="firstName" value="Kakumanu"></property>
		<property name="lastName" value="Manasa"></property>
	</bean>
	<bean class="com.cg.service.Customer" id="cus">
		<constructor-arg name="cid" value="1001"/>
		<constructor-arg name="fname" value="Nakirikanti"/>
		<constructor-arg name="lname" value="Bava"/>	
	</bean>
	
	
	<context:component-scan base-package="com.cg"/>
	<context:annotation
</beans>
